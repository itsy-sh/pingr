{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --serial clean make --parallel watch:bs serve:parcel",
    "serve:parcel": "parcel serve -p 8080 --hmr-port 8081 index.html",
    "clean": "npm-run-all clean:*",
    "clean:css": "rm -rf ./lib/css",
    "clean:js": "rm -rf ./lib/js",
    "clean:bs": "bsb -clean-world && rm -rf ./lib/bs && rm -rf .bsb.lock",
    "clean:build": "rm -rf ./build",
    "clean:dist": "rm -rf ./dist",
    "make": "npm-run-all make:*",
    "make:bs": "bsb -make-world",
    "make:css": "postcss src/tailwind.css -o lib/css/main.css",
    "watch:bs": "bsb -make-world -w < /dev/zero",
    "watch:css": "postcss src/tailwind.css --watch -o lib/css/main.css",
    "watch:parcel": "parcel watch index.html",
    "build": "npm-run-all --serial clean build:css build:bs build:parcel",
    "build:bs": "bsb -make-world",
    "build:css": "postcss src/tailwind.css --env production -o lib/css/main.css",
    "build:parcel": "parcel build --no-source-maps --out-dir build --public-url / --target browser index.html"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "bs-platform": "^8.0.3",
    "bsb-js": "^1.1.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "@types/react": "^16.8.6",
    "bs-fetch": "^0.6.1",
    "bs-rsuite-ui-react": "^1.2.2",
    "es6-promise": "^4.2.8",
    "highcharts": ">=6.0.0",
    "highcharts-react-official": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": ">=16.8.0",
    "react-dom": "^16.8.0",
    "reason-react": "^0.9.1",
    "style-loader": "^0.23.1"
  }
}
